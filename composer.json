{
    "name": "dew/my-framework",
    "description": "A modern PHP framework built from scratch",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Dew\\MyFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dew\\MyFramework\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dewunmi",
            "email": "adewunmialaba02@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "MIT"
}